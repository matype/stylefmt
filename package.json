{
  "name": "stylefmt",
  "version": "4.2.3",
  "description": "stylefmt is a tool that automatically formats CSS according to stylelint rules",
  "main": "index.js",
  "scripts": {
    "test": "tape test/*.js",
    "lint": "eslint -c .eslintrc.json index.js lib bin"
  },
  "bin": {
    "stylefmt": "bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/morishitter/stylefmt/git"
  },
  "engines": {
    "node": ">=4.2.0"
  },
  "keywords": [
    "css",
    "format",
    "formatter",
    "code style",
    "beautifier",
    "postcss-plugin",
    "postcss",
    "stylelint"
  ],
  "author": "Masaaki Morishita",
  "license": "MIT",
  "dependencies": {
    "cosmiconfig": "^1.1.0",
    "css-color-list": "0.0.1",
    "editorconfig": "^0.13.2",
    "minimist": "^1.2.0",
    "object-assign": "^4.1.0",
    "postcss": "^5.0.21",
    "postcss-scss": "^0.1.8",
    "postcss-sorting": "^1.6.1",
    "recursive-readdir": "^2.0.0",
    "repeat-string": "^1.5.4",
    "resolve-from": "^2.0.0",
    "stdin": "0.0.1",
    "tmp": "0.0.28"
  },
  "devDependencies": {
    "each-series": "^1.0.0",
    "eslint": "^3.3.1",
    "klaw": "^1.3.0",
    "tape": "^4.6.0"
  }
}
